}Vert;

typedef Vert *ind0;


vert criav(char mat[][], int i, int j){
    Vert *aux;
    
    aux=(Vert *)malloc(sizeof(Vert));
    
    if(mat[i+1][j]=="."){
        aux->direita=criav(mat,i+1,j);
    }
    if(mat[i-1][j]=="."){
        aux->esquerda=criav(mat,i-1,j);
    }
    if(mat[i][j+1]=="."){
        aux->baixo=criav(mat,i,j+1);
    }
    if(mat[i][j-1]=="."){
        aux->cima=criav(mat,i,j-1);
    }


}

